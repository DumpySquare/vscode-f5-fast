{
	"name": "vscode-f5-fast",
	"displayName": "F5 Networks FAST",
	"description": "(F)5 Networks (A)pplication (S)ervices (T)emplate engine extension",
	"publisher": "DumpySquare",
	"version": "0.1.7",
	"keywords": [
		"f5",
		"f5networks",
		"AS3",
		"fast",
		"DO",
		"TS",
		"bigip",
		"big-ip",
		"Snippets"
	],
	"engines": {
		"vscode": "^1.42.0"
	},
	"bugs": {
		"url": "https://github.com/DumpySquare/vscode-f5-fast/issues"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/DumpySquare/vscode-f5-fast/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/DumpySquare/vscode-f5-fast.git"
	},
	"categories": [
		"Formatters",
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:chuckJoke",
		"onCommand:f5.remoteCommand",
		"onCommand:f5.openSettings",
		"onCommand:f5.connectDevice",
		"onView:f5Hosts"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "json",
				"path": "./src/snippets.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "f5-fast-view-container",
					"title": "BIG-IPs",
					"icon": "images/f5_white_24x24.svg"
				}
			]
		},
		"views": {
			"f5-fast-view-container": [
				{
					"id": "f5Hosts",
					"name": "F5 Hosts"
				},
				{
					"id": "fastTemplates",
					"name": "FAST Templates"
				},
				{
					"id": "as3Tenants",
					"name": "AS3 Tenants"
				},
				{
					"id": "as3Tasks",
					"name": "AS3 Tasks"
				},
				{
					"id": "tsExamples",
					"name": "TS Examples"
				}
			]
		},
		"configuration": [
			{
				"title": "f5-fast",
				"properties": {
					"f5.hosts": {
						"type": "array",
						"items": {
							"type": "string",
							"description": "BIG-IP/Fast Host",
							"pattern": "^([^@]+@)?[a-z0-9-.]+(:[0-9]+)?$"
						},
						"default": [
							"admin@192.168.1.245"
						],
						"description": "Connection details for BIG-IP/FAST hosts in \"<user>@<host/ip>\" format"
					}
				}
			}
		],
		"commands": [
			{
				"command": "f5.connectDevice",
				"title": "Connect to Device",
				"category": "F5"
			},
			{
				"command": "f5.disconnect",
				"title": "Disconnect from Device",
				"category": "F5"
			},
			{
				"command": "f5.clearPasswords",
				"title": "Disconnect and Clear all Passwords",
				"category": "F5"
			},
			{
				"command": "f5.getF5HostInfo",
				"title": "Show Device Info",
				"category": "F5"
			},
			{
				"command": "f5.addHost",
				"title": "Add Host",
				"category": "F5"
			},
			{
				"command": "f5.editHost",
				"title": "Edit Host",
				"category": "F5"
			},
			{
				"command": "f5.removeHost",
				"title": "Remove Host",
				"category": "F5"
			},
			{
				"command": "f5.remoteCommand",
				"title": "Remote Command Execute",
				"category": "F5"
			},
			{
				"command": "f5.openSettings",
				"title": "Settings",
				"category": "F5"
			},
			{
				"command": "f5.refreshHostsTree",
				"title": "Refresh Devices",
				"category": "F5",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "f5-fast.getFastInfo",
				"title": "Show Info",
				"category": "F5-Fast"
			},
			{
				"command": "f5-fast.refreshTemplates",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "f5-as3.postDec",
				"title": "Post Declaration",
				"category": "F5-AS3"
			},
			{
				"command": "f5-as3.deleteTenant",
				"title": "Delete Tenant",
				"category": "F5-AS3"
			},
			{
				"command": "f5-as3.refreshTenantsTree",
				"title": "Refresh Tenants Tree",
				"category": "F5-AS3",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "f5-as3.refreshTasksTree",
				"title": "Refresh Tasks Tree",
				"category": "F5-AS3",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "f5-do.inspect",
				"title": "Inspect",
				"category": "F5-DO"
			},
			{
				"command": "f5-do.getDec",
				"title": "Get Current Declaration",
				"category": "F5-DO"
			},
			{
				"command": "f5-do.getTasks",
				"title": "Get Tasks",
				"category": "F5-DO"
			},
			{
				"command": "f5-do.postDec",
				"title": "Post Declaration",
				"category": "F5-DO"
			},
			{
				"command": "f5-ts.info",
				"title": "Show Info",
				"category": "F5-TS"
			},
			{
				"command": "f5-ts.getDec",
				"title": "Get Current Declaration",
				"category": "F5-TS"
			},
			{
				"command": "f5-ts.postDec",
				"title": "Post Declaration",
				"category": "F5-TS"
			},
			{
				"command": "f5-ts.enableTsExamples",
				"title": "Populate Examples Tree",
				"category": "F5-TS"
			},
			{
				"command": "writeMemento",
				"title": "writeMemento"
			},
			{
				"command": "readMemento",
				"title": "readMemento"
			},
			{
				"command": "f5-ts.enableTsExamples",
				"title": "Enable"
			},
			{
				"command": "f5-ts.disableTsExamples",
				"title": "Disable"
			},
			{
				"command": "f5.editHost",
				"title": "Edit",
				"icon": {
					"light": "resources/light/edit.svg",
					"dark": "resources/dark/edit.svg"
				}
			},
			{
				"command": "f5.removeHost",
				"title": "Delete",
				"category": "F5"
			},
			{
				"command": "chuckJoke",
				"title": "Chuck Joke"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "f5.addHost",
					"when": "view == f5Hosts",
					"group": "navigation"
				},
				{
					"command": "f5.refreshHostsTree",
					"when": "view == f5Hosts",
					"group": "navigation"
				},
				{
					"command": "f5-as3.refreshTenantsTree",
					"when": "view == as3Tenants",
					"group": "navigation"
				},
				{
					"command": "f5-as3.refreshTasksTree",
					"when": "view == as3Tasks",
					"group": "navigation"
				},
				{
					"command": "f5-fast.refreshTemplates",
					"when": "view == fastTemplates",
					"group": "navigation"
				},

				{
					"command": "f5-ts.enableTsExamples",
					"when": "view == tsExamples",
					"group": "navigation"
				},
				{
					"command": "f5-ts.disableTsExamples",
					"when": "view == tsExamples",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "f5.editHost",
					"when": "view == f5Hosts",
					"group": "inline"
				},
				{
					"command": "f5.removeHost",
					"when": "view == f5Hosts"
				},
				{
					"command": "f5.getF5HostInfo",
					"when": "view == f5Hosts"
				},
				{
					"command": "f5-as3.deleteTenant",
					"when": "view == as3Tenants"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.37",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"fs": "0.0.1-security",
		"https": "^1.0.0",
		"keytar": "^5.6.0",
		"vsce": "^1.75.0"
	}
}
