{
	"name": "vscode-f5-fast",
	"displayName": "F5 Networks FAST",
	"description": "(F)5 Networks (A)pplication (S)ervices (T)emplate engine extension",
	"publisher": "DumpySquare",
	"version": "2.4.0-b",
	"keywords": [
		"f5",
		"f5networks",
		"AS3",
		"FAST",
		"DO",
		"TS",
		"bigip",
		"big-ip",
		"Snippets"
	],
	"engines": {
		"vscode": "^1.42.0"
	},
	"bugs": {
		"url": "https://github.com/DumpySquare/vscode-f5-fast/issues"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DCDCDC",
		"theme": "light"
	},
	"homepage": "https://github.com/DumpySquare/vscode-f5-fast/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/DumpySquare/vscode-f5-fast.git"
	},
	"categories": [
		"Formatters",
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "json",
				"path": "./snippets.json"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "*.as3.json",
				"url": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema.json"
			},
			{
				"fileMatch": "*.do.json",
				"url": "https://raw.githubusercontent.com/F5Networks/f5-declarative-onboarding/master/src/schema/latest/base.schema.json"
			},
			{
				"fileMatch": "*.ts.json",
				"url": "https://raw.githubusercontent.com/F5Networks/f5-telemetry-streaming/master/src/schema/latest/base_schema.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "f5-view-container",
					"title": "BIG-IPs",
					"icon": "images/f5_white_24x24.svg"
				}
			]
		},
		"views": {
			"f5-view-container": [
				{
					"id": "f5Hosts",
					"name": "F5 Hosts"
				},
				{
					"id": "fastView",
					"name": "FAST View",
					"when": "f5.fastInstalled"
				},
				{
					"id": "as3Tenants",
					"name": "AS3",
					"when": "f5.as3Installed"
				},
				{
					"id": "as3Tasks",
					"name": "iRules/iApps",
					"when": "f5.tcl"
				},
				{
					"id": "decExamples",
					"name": "Documentation/Examples"
				},
				{
					"id": "cfgTree",
					"when": "f5.cfgTreeContxt",
					"name": "Config Explorer"
				}
			]
		},
		"configuration": [
			{
				"title": "f5-fast",
				"properties": {
					"f5.hosts": {
						"type": "array",
						"description": "F5 FAST extension device details",
						"items": {
							"type": "object",
							"properties": {
								"device": {
									"type": "string",
									"description": "Connection details for BIG-IP/FAST hosts in \"<user>@<host/ip>\" format",
									"pattern": "^([^@]+@)?[a-z0-9-.]+(:[0-9]+)?$",
									"examples": [
										"admin@192.168.1.254",
										"usr1@10.40.40.20:8443",
										"abc4312@dev.test.net"
									]
								},
								"provider": {
									"type": "string",
									"description": "logonProviderName to define remote auth service - ONLY NEEDED IF NOT DEFAULT 'local'",
									"examples": [
										"local",
										"radius",
										"TACACS",
										"ldap",
										"custom"
									]
								},
								"onConnect": {
									"type": "array",
									"description": "List of terminal/bash commands to execute at connect",
									"examples": [
										"ssh admin@192.168.1.254",
										"ssh ${this.device}"
									]
								},
								"onDisconnect": {
									"type": "array",
									"description": "List of terminal/bash commands to execute at disconnect"
								}
							},
							"required": [
								"device"
							],
							"default": "admin@192.168.1.254"
						}
					},
					"f5.as3Post.async": {
						"type": "boolean",
						"default": true,
						"description": "Make as3 posts async by default"
					},
					"f5.showAuthTokenTimer": {
						"type": "boolean",
						"default": false,
						"description": "Displays a countdown of the auth token lifetime.  Extension will refresh the token as needed with the next call.  This is mainly for visibility/debugging purposes, or for those like a ton of feedback"
					},
					"f5.asyncInterval": {
						"type": "number",
						"default": 5,
						"description": "Number of seconds to wait between checking POST task status"
					},
					"f5.timeoutinmilliseconds": {
						"type": "integer",
						"default": 0,
						"scope": "resource",
						"description": "*DEV-beta-Preview* Timeout in milliseconds. 0 for infinity"
					},
					"f5.newEditorTabForAll": {
						"type": "boolean",
						"default": false,
						"scope": "resource",
						"description": "Show everything in a new editor tab"
					},
					"f5.enableWebViews": {
						"type": "boolean",
						"default": false,
						"scope": "resource",
						"description": "*DEV-inProgress*"
					},
					"f5.newEditorColumn": {
						"type": "string",
						"enum": [
							"current",
							"beside"
						],
						"markdownEnumDescriptions": [
							"Open new editor tab in the column of current editor tab",
							"Open editor tab to the side of the current active column and the side direction depends on `workbench.editor.openSideBySideDirection` setting, either right or below the current editor column"
						],
						"default": "beside",
						"scope": "resource",
						"description": "Display new editor in current column or beside"
					},
					"f5.httpResponseDetails": {
						"type": "string",
						"enum": [
							"exchange",
							"full",
							"body"
						],
						"enumDescriptions": [
							"Entire HTTP exchange including both request/response headers + body",
							"Response headers + body",
							"Response body only"
						],
						"default": "body",
						"scope": "resource",
						"description": "HTTP Response details (if any) shown in editor tab"
					},
					"f5.preserveEditorFocus": {
						"type": "boolean",
						"default": false,
						"scope": "resource",
						"description": "Prevent new editor tab from taking focus when opened or updated\n So, by default(disabled), each new editor that is opened and/or updated with also take focus"
					},
					"f5.tcl": {
						"type": "boolean",
						"default": false,
						"scope": "resource",
						"description": "Enable tcl/irule/iapp functionality."
					},
					"f5.logLevel": {
						"type": "string",
						"enum": [
							"error",
							"warn",
							"info",
							"verbose"
						],
						"default": "error",
						"scope": "resource",
						"description": "*DEV-beta-Preview* The verbosity of logging in the REST output panel"
					}
				}
			}
		],
