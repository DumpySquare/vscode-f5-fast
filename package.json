		"menus": {
			"explorer/context": [
				{
						"when": "f5.fastInstalled && explorerResourceIsFolder",
						"command": "f5-fast.postTemplateSet",
						"group": "F5-Fast"
				},
				{
						"when": "f5.fastInstalled && resourceExtname == .mst",
						"command": "f5-fast.postTemplate",
						"group": "F5-Fast"
				}
			],
			"view/title": [
				{
					"command": "f5.addHost",
					"when": "view == f5Hosts",
					"group": "navigation"
				},
				{
					"command": "f5.refreshHostsTree",
					"when": "view == f5Hosts",
					"group": "navigation"
				},
				{
					"command": "f5-as3.refreshTenantsTree",
					"when": "view == as3Tenants",
					"group": "navigation"
				},
				{
					"command": "f5-as3.refreshTasksTree",
					"when": "view == as3Tasks",
					"group": "navigation"
				},
				{
					"command": "f5-fast.refreshTemplates",
					"when": "view == fastView",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"command": "f5.disconnect",
					"when": "f5.device"
				},
				{
					"command": "f5.getF5HostInfo",
					"when": "f5.device"
